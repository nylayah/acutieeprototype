{
  "expo": {
    "name": "Acutie",
    "slug": "Acutie",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./App/assets/FitAll.png",
    "splash": {
      "image": "./App/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#a8dbdcff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./App/assets/adaptive-icon.png",
        "backgroundColor": "#a8dbdcff"
      }
    },
    "web": {
      "favicon": "./App/assets/FitAll.png"
    }
  }
}
